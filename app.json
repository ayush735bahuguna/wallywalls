{
  "expo": {
    "name": "wallywalls",
    "slug": "wallywalls",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/favicon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ayushbahuguna1122.wallywalls"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "52df6c49-911a-4117-9a57-35bc6fba7111"
      }
    },
    "owner": "ayushbahuguna1122",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/52df6c49-911a-4117-9a57-35bc6fba7111"
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Wallywalls to access your photos.",
          "savePhotosPermission": "Allow Wallywalls to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ]
  }
}